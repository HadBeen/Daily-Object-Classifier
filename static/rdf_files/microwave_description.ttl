@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.org/heat_food> rdfs:label "heat food" .

<http://example.org/microwave> a owl:Class ;
    rdfs:label "microwave" ;
    rdfs:seeAlso <http://example.org/heat_food>,
        <http://example.org/microwave>,
        <http://example.org/the_microwave> .

<http://example.org/the_microwave> rdfs:label "the microwave" .

